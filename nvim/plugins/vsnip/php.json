{
    "html": {
        "prefix": "html",
        "body": [
            "<!doctype html>",
            "<html>",
            "",
            "<head>",
            "<meta charset=utf-8>",
            "<title></title>",
            "</head>",
            "",
            "<body>",
            "<header></header>",
            "<main></main>",
            "<footer></footer>",
            "</body>",
            "",
            "</html>"
        ]
    },
    "declare": {
        "prefix": "declare",
        "body": "declare(strict_types=1);"
    },
    "php": {
        "prefix": "php",
        "body": "<?php $1 ?>"
    },
    "div": {
        "prefix": "div",
        "body": "<div>$1</div>"
    },
    "var_dump": {
        "prefix": "vardump",
        "body": [
            "echo \"<pre>\";",
            "var_dump($1);",
            "echo \"</pre>\";"
        ]
    },
    "function": {
        "prefix": "function",
        "body": "(function ():$1 {$2})();"
    }
}
